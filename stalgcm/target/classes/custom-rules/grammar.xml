<?xml version="1.0" encoding="UTF-8"?>
<rules lang="tl-PH" xmlns="http://www.languagetool.org/xml/rules">

    <rule id="REPEATED_AT" name="Repeated 'at' words">
        <category id="FILIPINO_STYLE" name="Filipino Style and Grammar"/>
        <pattern>
            <token>at</token>
            <token>at</token>
        </pattern>
        <message>Ulit-ulit ang salitang 'at'.</message>
        <example correction="at">Siya ay nagpunta <marker>at at</marker> nagtrabaho.</example>
        <example>Siya ay nagpunta at nagtrabaho.</example>
    </rule>

</rules>
